ALGORITHM CP_alg2
VAR
    C1 : ARRAY_OF INTEGER[5];
    C2 : ARRAY_OF INTEGER[5];
    i:=INTEGER;
    j:=INTEGER;
    S:= BOOLEAN;
BEGIN
sum:= 0 ; 
REPEAT
 FOR i FROM 0 TO tab.length-1 STEP 1  DO
 write C1
    FOR j FROM 0 TO tab.length-1 STEP 1  DO
    write C2
    IF (C1[i] <> C2[j] ) then
    S:= true  
    ELSE
    S:= false
    END_IF
    END_FOR
IF S:= true THEN
sum:= C1 [i] + C2 [j]
ELSE 
sum:= 0
END_FOR
write ("Sum of overlapping elements:"+Sum);
END CP_alg2